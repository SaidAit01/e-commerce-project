{
  "name": "@wix/error-handler",
  "version": "1.67.0",
  "license": "MIT",
  "author": {
    "name": "kobia",
    "email": "kobia@wix.com"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types",
    "!dist/**/__tests__",
    "exports",
    "testkit"
  ],
  "scripts": {
    "start": "yarn build:translations && yoshi-library start",
    "build": "yarn build:translations && yoshi-library build",
    "build:translations": "yarn build:translations:types & yarn build:translations:imports",
    "build:translations:imports": "ts-node --project tsconfig.scripts.json -T scripts/generate-translations-imports.ts && npx prettier src/services/translationsImportsMap.ts --write",
    "build:translations:types": "ts-node --project tsconfig.scripts.json -T scripts/generate-keys.ts && npx prettier src/services/translations.ts --write",
    "test:unit": "yoshi-library test",
    "lint": "yoshi-library lint",
    "type-check": "tsc --noEmit",
    "type-check:tests": "tsc --noEmit -p tsconfig.tests.json",
    "build:concat-docs": "ts-node --project tsconfig.scripts.json -T scripts/concat-docs.ts"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.2",
    "@wix/error-handler-core": "1.19.0",
    "events": "^3.3.0",
    "http-status-codes": "^2.3.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@formatjs/icu-messageformat-parser": "^2.11.2",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^27.5.2",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "@wix/ambassador-bookings-categories-v2-category": "^1.0.31",
    "@wix/ambassador-bookings-v1-booking-policy": "^1.0.92",
    "@wix/design-system": "^1.189.1",
    "@wix/eslint-config-yoshi": "^6.141.0",
    "@wix/essentials": "^0.1.25",
    "@wix/fe-essentials": "^1.842.0",
    "@wix/fe-essentials-standalone": "^1.1323.0",
    "@wix/jest-yoshi-preset-base": "^6.141.0",
    "@wix/sdk": "^1.15.26",
    "@wix/sdk-runtime": "^0.3.59",
    "@wix/sdk-testkit": "^0.1.6",
    "@wix/stores": "^1.0.471",
    "@wix/wix-ui-icons-common": "^3.73.0",
    "@wix/wix-ui-test-utils": "^1.3.0",
    "@wix/yoshi-flow-library": "^6.141.0",
    "fetch-mock": "^12.5.3",
    "globby": "^11.1.0",
    "i18next": "^19.9.2",
    "i18next-icu": "^2.3.0",
    "intl-messageformat": "^7.8.4",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "react": "^16.14.0",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@wix/fe-essentials": "^1.233.0",
    "i18next": ">=19.9.2",
    "i18next-icu": ">=2.3.0",
    "intl-messageformat": ">=7.8.4"
  },
  "peerDependenciesMeta": {
    "@wix/fe-essentials": {
      "optional": true
    },
    "i18next": {
      "optional": true
    },
    "i18next-icu": {
      "optional": true
    },
    "intl-messageformat": {
      "optional": true
    }
  },
  "jest": {
    "preset": "@wix/jest-yoshi-preset-base",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": [
        "$1.js",
        "$1.ts"
      ]
    }
  },
  "eslintConfig": {
    "extends": "@wix/eslint-config-yoshi",
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true,
          "trailingComma": "all"
        },
        {
          "usePrettierrc": false
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "src/**/*.ts*"
        ],
        "rules": {
          "no-restricted-imports": [
            "error",
            {
              "patterns": [
                {
                  "group": [
                    "src"
                  ]
                }
              ]
            }
          ],
          "no-console": [
            "error",
            {
              "allow": [
                "warn",
                "error"
              ]
            }
          ]
        }
      }
    ]
  },
  "yoshiFlowLibrary": {
    "buildEsmWithBabel": true
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress",
      "artifactId": "error-handler"
    },
    "validations": {
      "postBuild": [
        "type-check:tests",
        "test:unit"
      ],
      "postDependenciesBuild": [
        "lint"
      ]
    }
  },
  "wallaby": {
    "autoDetect": true
  },
  "falconPackageHash": "0e6a07e16b13d055af57e805e32e50b115e6ada876e7845e694c1dcd"
}